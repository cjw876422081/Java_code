package Student;

import com.sun.jndi.url.iiopname.iiopnameURLContextFactory;
import com.sun.swing.internal.plaf.basic.resources.basic;

public class TestStudnet {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student A = new Student(1 , "张三" , "男" , 10 , 100.0);
		A.print();
		Student B = new Student();
		B.setsNo(2);
		B.setsName("王五");
		B.setsSex("男");
		B.setsAge(15);
		B.setsJava(99.0);
		B.print();
		Student C = new Student(11 , 60 );
		C.setsNo(3);
		C.setsName("赵六");
		C.print();
		Student D = new Student("女" ,11 , 60 );
		D.setsNo(4);
		D.setsName("张辉");
		D.print();
		Student E = new Student("李四" , "男", 16 , 60 );
		E.setsNo(5);
		E.print();
		double[] sz = {0 , 0 , 0 , 0 , 0};
		sz[0] = A.getsJava() ; 
		sz[1] = B.getsJava() ; 
		sz[2] = C.getsJava() ; 
		sz[3] = D.getsJava() ; 
		sz[4] = E.getsJava() ; 
		double sum = 0 , max= 0  , min= 0  ;
		int i = 0 ; 
		while( i < 5 ) {
			sum += sz[i] ;
			if( max <= sz[i]) {
				max = sz[i] ;
			}
			if(min >= sz[i]) {
				min = sz[i] ; 
			}
			i ++ ;  
		}
		System.out.println("平均成绩" + (sum/5) + "最高成绩" + max + "最低成绩" + min);
				
		
	}

}
